<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.itcelaya.cosmosexplorerdemo.controllers.spaceX.LoginSpaceXController">
   <children>
      <ImageView fx:id="imageviewAPI" fitHeight="700.0" fitWidth="700.0" pickOnBounds="true" />
      <VBox alignment="CENTER" maxHeight="850.0" maxWidth="1.7976931348623157E308" prefHeight="588.0" prefWidth="636.0" styleClass="background">
         <VBox alignment="CENTER" maxWidth="1.7976931348623157E308" spacing="8.0" style="-fx-background-color: rgba(255, 255, 255, 0.3);">
            <children>
                   <Label alignment="CENTER" graphicTextGap="8.0" styleClass="title" text="Log in" textAlignment="CENTER" />
               <Button fx:id="singUpBtn" mnemonicParsing="false" onAction="#onShowSignUp" text="Sign up?" />
               <Label fx:id="messageLabel" alignment="CENTER" maxWidth="1.7976931348623157E308" styleClass="message" text="Enter user name and password:" wrapText="true" />
            </children>
            <VBox.margin>
               <Insets />
            </VBox.margin>
         </VBox>
         <VBox fx:id="vbContainer" alignment="CENTER" maxHeight="1.7976931348623157E308">
            <VBox.margin>
               <Insets />
            </VBox.margin>
            <padding>
               <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
            </padding>
         </VBox>
              <Button fx:id="loginBtn" onAction="#onSendClick" styleClass="btn-success" text="Log In" />
      </VBox>
   </children>
</StackPane>
